Print palindrome or not

import java.util.*;
import java.lang.*;


public class Main {
  public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      String s=sc.next();
      String str="";
      int l=s.length();
      for(int i=l-1;i>=0;i--){
        str=str+s.charAt(i);
    }
    if(s.equals(str)){
      System.out.println("palindrome");
    }
    else{
      System.out.println("not palindrome");
    }
  }
}
